<info xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="https://apps.nextcloud.com/schema/apps/info.xsd">
    <id>zimbradrive</id>
    <name>Zimbra Drive</name>
    <summary>Integration with Zimbra</summary>
    <description>
        <![CDATA[Add Nextcloud / ownCloud as Briefcase in Zimbra with the full features of both applications.]]></description>
    <licence>mit</licence>
    <author>ZeXtras</author>
    <version>0.8.10</version>
    <namespace>ZimbraDrive</namespace>
    <category>auth</category>
    <category>integration</category>
    <dependencies>
        <owncloud min-version="9" max-version="9"/>
        <nextcloud min-version="9" max-version="11"/>
    </dependencies>
    <types>
        <authentication/>
        <prelogin/>
    </types>
    <settings>
        <admin>OCA\ZimbraDrive\Settings\Admin</admin>
        <admin-section>OCA\ZimbraDrive\Settings\Section</admin-section>
    </settings>
</info>
